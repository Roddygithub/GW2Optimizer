# Backend
ENVIRONMENT=staging
DATABASE_URL=sqlite+aiosqlite:///./staging.db
REDIS_URL=redis://localhost:6379/1
SECRET_KEY=staging-secret-key-123
FRONTEND_URL=http://localhost:5174
LOG_LEVEL=INFO
LOG_FILE=logs/staging.log

# API Configuration
API_VERSION=v1
API_V1_STR=/api/v1
API_PREFIX=/api/v1

# Authentication
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# AI Configuration
AI_MODEL_NAME=gpt-4
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2048
AI_TIMEOUT=2.0
AI_RATE_LIMIT=60
AI_FALLBACK_ENABLED=true

# Frontend
VITE_API_URL=http://localhost:8001
VITE_ENV=staging

# Feature Flags
AI_CORE_ENABLED=true
ML_TRAINING_ENABLED=false
